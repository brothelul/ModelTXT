.application-cell{
  background-color: #fff;
  padding:10px 15px;
  position:relative;
  display:-webkit-box;
  display:-webkit-flex;
  display:flex;
  -webkit-box-align:center;
  -webkit-align-items:center;
  align-items:center;
  font-size: 15px;
  border-bottom: 1px solid #F8F8F8;
}
.application-cell-header{
  position:relative;
  margin-right:10px;
}
.application-cell-header image{
  width: 50px;
  height: 50px;
  display: block;
}
.application-cell-header-comment{
  font-size: 13px;
  color: #888888;
  margin-top: 6px;
}
.application-cell-body{
  -webkit-box-flex:1;
  -webkit-flex:1;
  flex:1;
}
.no-data view{
  position:relative;
  top:-.9em;
  padding:0 .55em;
  background-color:#fff;
  color:#999;
  display:inline-block;
  vertical-align:middle;
}
.no-data{
  border-top:1px solid #e5e5e5;
  margin-top:20em;
  width:65%;
  margin:1.5em auto;
  line-height:1.6em;
  font-size:14px;
  text-align:center;
}